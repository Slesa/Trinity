<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary
            Source="pack://application:,,,/HighRise.View;component/Resources/Base/BaseValueResources.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="Brush.Border.Building.Visual" po:Freeze="True" Color="{StaticResource Color.White}" />
    <SolidColorBrush x:Key="Brush.Border.Room.Visual" Opacity="{StaticResource Opacity.Border.Building.Visual}" po:Freeze="True" Color="{StaticResource Color.White}" />
    <SolidColorBrush x:Key="Brush.Border.Floor.Visual" Opacity="{StaticResource Opacity.Border.Building.Visual}" po:Freeze="True" Color="{StaticResource Color.White}" />

    <SolidColorBrush x:Key="Brush.Background.Room.Visual.Error" Opacity="1" po:Freeze="True" Color="{StaticResource Color.Red40}" />
    <SolidColorBrush x:Key="Brush.Room.Visual.Light.On" po:Freeze="True" Color="{StaticResource Color.Yellow50}" />

</ResourceDictionary>